<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="568.0" prefWidth="720.0">
        <children>
            <!-- Botão para acessar o serviço de quarto -->
            <Button fx:id="btnServicoQuarto" layoutX="266.0" layoutY="40.0" mnemonicParsing="false" text="Serviço de Quarto" />
            
            <!-- Botão para sair do sistema -->
            <Button fx:id="btnSair" layoutX="654.0" layoutY="40.0" mnemonicParsing="false" text="Sair" />
            
            <!-- Botão para cadastrar um novo cliente -->
            <Button fx:id="btnCadastrarCliente" layoutX="21.0" layoutY="415.0" mnemonicParsing="false" text="Cadastrar Cliente" />
            
            <!-- Botão para visualizar os quartos ocupados -->
            <Button fx:id="btnQuartosOcupados" layoutX="132.0" layoutY="40.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="124.0" text="Quartos Ocupados" />
            
            <!-- Botão para visualizar os quartos livres -->
            <Button fx:id="btnQuartosLivres" layoutX="23.0" layoutY="40.0" mnemonicParsing="false" text="Quartos Livres" />
            
            <!-- Botão para cadastrar um novo quarto -->
            <Button fx:id="btnCadastrarQuarto" layoutX="21.0" layoutY="495.0" mnemonicParsing="false" text="Cadastrar Quarto" />
            
            <!-- Botão para cadastrar um novo serviço -->
            <Button fx:id="btnCadastrarServico" layoutX="21.0" layoutY="455.0" mnemonicParsing="false" text="Cadastrar Serviço" />
        </children>
    </AnchorPane>
</Scene>
